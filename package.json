{
  "name": "express-api-typeorm",
  "version": "5.2.0",
  "description": "A simple Express API with TypeScript, TypeORM, and PostgreSQL",
  "main": "src/main.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "nodemon --exec node ./dist/main.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc && tsc-alias",
    "dev": "concurrently \"npm run build:watch\" \"npm run watch-dev\"",
    "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/main.js",
    "start:staging": "NODE_ENV=staging pm2-runtime ./dist/main.js",
    "start:production": "NODE_ENV=production pm2-runtime ./dist/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create": "node ./dist/app/database/create-db.js",
    "db:drop": "node ./dist/app/database/drop-db.js",
    "db:schema-sync": "npx typeorm schema:sync -d ./dist/app/database/connection.js",
    "db:schema-drop": "npx typeorm schema:drop -d ./dist/app/database/connection.js",
    "db:reset": "npm-run-all db:schema-drop db:schema-sync",
    "db:sync": "npm-run-all db:create db:reset",
    "prerelease": "npm-run-all prebuild",
    "postrelease": "git push --follow-tags origin main",
    "release": "release-it",
    "release:pre": "release-it --preRelease=beta",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major"
  },
  "keywords": [
    "express",
    "api",
    "typescript"
  ],
  "author": "masb0ymas <me@masb0ymas.com>",
  "license": "MIT",
  "engines": {
    "node": ">=20.x"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.779.0",
    "@aws-sdk/s3-request-presigner": "^3.779.0",
    "@google-cloud/storage": "^7.15.2",
    "argon2": "^0.41.1",
    "colorette": "^2.0.20",
    "compression": "^1.8.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "lodash": "^4.17.21",
    "minio": "^8.0.5",
    "multer": "1.4.5-lts.1",
    "npm-run-all": "^4.1.5",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "pm2-runtime": "^5.4.1",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.21",
    "typeorm-extension": "^3.7.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.16",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.14",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "release-it": "^18.1.2",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.13",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  }
}
