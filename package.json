{
  "name": "expresso-typeorm",
  "version": "0.1.0",
  "description": "expresso typeorm, postgesql, redis, aws s3",
  "main": "server.ts",
  "scripts": {
    "clean": "rimraf dist coverage",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "prebuild": "npm-run-all clean lint typecheck",
    "build": "npx swc src -d dist",
    "start": "nodemon --exec node ./dist/server.js",
    "dev": "yarn && npm-run-all build start",
    "db:sync": "npx typeorm schema:sync -d ./dist/database/data-source.js",
    "db:drop": "npx typeorm schema:drop -d ./dist/database/data-source.js",
    "db:reset": "yarn && npm-run-all db:drop db:sync",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "author": "masb0ymas",
  "license": "MIT",
  "engines": {
    "node": ">= 14.x"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.267.0",
    "@aws-sdk/s3-request-presigner": "^3.267.0",
    "@google-cloud/storage": "^6.9.2",
    "axios": "^1.3.2",
    "bcrypt": "^5.1.0",
    "chalk": "~4.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^6.7.0",
    "express-useragent": "^1.0.15",
    "googleapis": "^111.0.0",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "i18next": "^22.4.9",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-middleware": "^3.2.2",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "libphonenumber-js": "^1.10.19",
    "lodash": "^4.17.21",
    "minio": "^7.0.32",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "nodemailer-mailgun-transport": "^2.1.5",
    "npm-run-all": "^4.1.5",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "request-ip": "^3.3.0",
    "slugify": "^1.6.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.2",
    "@swc/cli": "^0.1.61",
    "@swc/core": "^1.3.35",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-useragent": "^1.0.2",
    "@types/hpp": "^0.2.2",
    "@types/ioredis-mock": "^8.2.1",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/minio": "^7.0.15",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.13.0",
    "@types/nodemailer": "^6.4.7",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "ioredis-mock": "^8.2.6",
    "jest": "^29.4.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "_moduleAliases": {
    "@core/*": "dist/@core/*",
    "@config/*": "dist/config/*",
    "@controllers/*": "dist/controllers/*",
    "@database/*": "dist/database/*",
    "@middlewares/*": "dist/middlewares/*",
    "@routes/*": "dist/routes/*"
  }
}
