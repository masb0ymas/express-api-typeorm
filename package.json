{
  "name": "expresso-typeorm",
  "version": "4.4.5",
  "description": "expressjs, typescript, typeorm, gcp, aws, minio",
  "main": "./dist/main.js",
  "scripts": {
    "key:generate": "node ./dist/scripts/generate.js",
    "clean": "rimraf dist coverage",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "prebuild": "npm-run-all clean lint typecheck",
    "build": "npx swc src -d dist",
    "build:ts": "tsc && tsc-alias",
    "start": "nodemon --exec node ./dist/main.js",
    "dev": "npm-run-all build start",
    "dev:watch": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "npx swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/main.js",
    "db:create": "node ./dist/scripts/createDB.js",
    "db:destroy": "node ./dist/scripts/dropDB.js",
    "db:sync": "npx typeorm schema:sync -d ./dist/database/data-source.js",
    "db:drop": "npx typeorm schema:drop -d ./dist/database/data-source.js",
    "db:reset": "npm-run-all db:drop db:sync",
    "deploy:pm2": "npm run build && npx pm2 reload ecosystem.config.js",
    "serve:staging": "NODE_ENV=staging node ./dist/main.js",
    "serve:production": "NODE_ENV=production node ./dist/main.js",
    "serve:staging-docker": "NODE_ENV=staging pm2-runtime ./dist/main.js",
    "serve:production-docker": "NODE_ENV=production pm2-runtime ./dist/main.js",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prerelease": "npm run prebuild",
    "postrelease": "git push --follow-tags origin main",
    "release": "standard-version",
    "release:pre": "npm run release -- --prerelease",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major"
  },
  "keywords": [
    "expressjs",
    "typescript",
    "typeorm",
    "gcp",
    "aws",
    "minio"
  ],
  "author": "masb0ymas <n.fajri@mail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 16.x"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "colorette": "^2.0.20",
    "compression": "^1.7.4",
    "concurrently": "^8.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^6.11.2",
    "express-useragent": "^1.0.15",
    "expresso-core": "^0.8.2",
    "expresso-hooks": "^0.8.2",
    "expresso-provider": "^0.8.2",
    "expresso-query": "^0.8.2",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "i18next": "^23.5.1",
    "i18next-fs-backend": "^2.2.0",
    "i18next-http-middleware": "^3.3.2",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "pino": "^8.15.1",
    "pino-http": "^8.5.0",
    "pino-pretty": "^10.2.0",
    "pm2": "^5.3.0",
    "qs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.17",
    "typeorm-extension": "^2.8.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "1.3.78",
    "@types/compression": "^1.7.3",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/express-useragent": "^1.0.2",
    "@types/hpp": "^0.2.2",
    "@types/lodash": "^4.14.198",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.6.1",
    "@types/node-cron": "^3.0.8",
    "@types/qs": "^6.9.8",
    "@types/request-ip": "^0.0.38",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "standard-version": "^9.5.0",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.2.2"
  }
}
