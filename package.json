{
  "name": "expresso-typeorm",
  "version": "1.0.0",
  "description": "Express built in TypeORM",
  "main": "./src/server.ts",
  "scripts": {
    "dev": "npm run dev:babel",
    "dev:ts": "nodemon --exec 'ts-node' ./src/server.ts",
    "dev:babel": "nodemon --exec 'babel-node' --extensions \".ts\" ./src/server.ts",
    "clean": "rm -rf ./dist",
    "test:lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "test:types": "tsc --noEmit",
    "before-build": "npm run clean && yarn && npm run test:lint && npm run test:types",
    "build:ts": "npm run before-build && tsc -p .",
    "build:babel": "npm run before-build && babel src --out-dir dist --extensions \".ts\",\".js\"",
    "serve:staging": "NODE_ENV=staging node ./dist/server.js",
    "serve:production": "NODE_ENV=production node ./dist/server.js",
    "db:create": "npx typeorm-extension db:create",
    "db:drop": "npx typeorm-extension db:drop",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "typeorm"
  ],
  "author": "masb0ymas",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "http-errors": "^1.8.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.37",
    "typeorm-extension": "^0.2.9",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@babel/runtime": "^7.15.4",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/http-errors": "^1.8.1",
    "@types/lodash": "^4.14.172",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}
