{
  "name": "expresso-typeorm",
  "version": "1.0.0",
  "description": "Express built in TypeORM",
  "main": "./src/server.ts",
  "scripts": {
    "key:generate": "npx ts-node ./src/@expresso/scripts/generate.ts",
    "dev": "nodemon --exec 'ts-node' ./src/server.ts",
    "start": "npm run dev",
    "clean": "rm -rf ./dist",
    "test:lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "test:types": "tsc --noEmit",
    "test:types-watch": "npm run test:types -- --watch",
    "prebuild": "npm run clean && yarn && npm run test:lint && npm run test:types",
    "build": "npx tsc -p .",
    "deploy": "npm run build && pm2 reload ecosystem.config.js",
    "serve:staging": "NODE_ENV=staging node ./dist/server.js",
    "serve:production": "NODE_ENV=production node ./dist/server.js",
    "serve:production-docker": "NODE_ENV=production pm2-runtime ./dist/server.js",
    "db:create": "npx typeorm-extension db:create",
    "db:drop": "npx typeorm-extension db:drop",
    "db:reset": "npm run db:drop && npm run db:create",
    "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "release": "standard-version",
    "release:pre": "npm run release -- --prerelease",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "typeorm"
  ],
  "author": "masb0ymas",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 12.x"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-useragent": "^1.0.15",
    "googleapis": "^87.0.0",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "http-errors": "^1.8.0",
    "i18next": "^21.0.1",
    "i18next-fs-backend": "^1.1.1",
    "ioredis": "^4.27.9",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.3",
    "mysql": "^2.18.1",
    "nodemailer": "^6.6.3",
    "nodemailer-mailgun-transport": "^2.1.3",
    "rate-limiter-flexible": "^2.2.4",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.37",
    "typeorm-extension": "^0.2.9",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yup": "0.31.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.1",
    "@types/hpp": "^0.2.2",
    "@types/http-errors": "^1.8.1",
    "@types/i18next-fs-backend": "^1.1.1",
    "@types/ioredis": "^4.27.4",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.172",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.7.13",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "@types/redis": "^2.8.32",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "_moduleAliases": {
    "@expresso/*": "dist/@expresso/*",
    "@config/*": "dist/config/*",
    "@controllers/*": "dist/controllers/*",
    "@entity/*": "dist/entity/*",
    "@jobs/*": "dist/jobs/*",
    "@middlewares/*": "dist/middlewares/*",
    "@migration/*": "dist/migration/*",
    "@routes/*": "dist/routes/*",
    "@seeds/*": "dist/seeds/*",
    "@utils/*": "dist/utils/*",
    "@views/*": "dist/views/*"
  }
}
