{
  "name": "expresso-typeorm",
  "version": "4.5.1",
  "description": "expressjs, typescript, typeorm, gcp, aws, minio",
  "main": "./dist/main.js",
  "scripts": {
    "key:generate": "node ./dist/scripts/generate.js",
    "clean": "rimraf dist coverage",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint \"*/**/*.{js,jsx,ts,tsx}\" --quiet --fix",
    "prebuild": "npm-run-all clean lint typecheck",
    "build": "npx swc src -d dist",
    "build:ts": "tsc && tsc-alias",
    "start": "nodemon --exec node ./dist/main.js",
    "dev": "npm-run-all build start",
    "dev:watch": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "npx swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/main.js",
    "db:create": "node ./dist/scripts/createDB.js",
    "db:destroy": "node ./dist/scripts/dropDB.js",
    "db:sync": "npx typeorm schema:sync -d ./dist/database/data-source.js",
    "db:drop": "npx typeorm schema:drop -d ./dist/database/data-source.js",
    "db:reset": "npm-run-all db:drop db:sync",
    "deploy:pm2": "npm run build && npx pm2 reload ecosystem.config.js",
    "serve:staging": "NODE_ENV=staging node ./dist/main.js",
    "serve:production": "NODE_ENV=production node ./dist/main.js",
    "serve:staging-docker": "NODE_ENV=staging pm2-runtime ./dist/main.js",
    "serve:production-docker": "NODE_ENV=production pm2-runtime ./dist/main.js",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prerelease": "npm run prebuild",
    "postrelease": "git push --follow-tags origin main",
    "release": "standard-version",
    "release:pre": "npm run release -- --prerelease",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major"
  },
  "keywords": [
    "expressjs",
    "typescript",
    "typeorm",
    "gcp",
    "aws",
    "minio"
  ],
  "author": "masb0ymas <n.fajri@mail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 16.x"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "colorette": "^2.0.20",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.3.1",
    "express-useragent": "^1.0.15",
    "expresso-core": "^0.11.6",
    "expresso-hooks": "^0.11.6",
    "expresso-provider": "^0.11.6",
    "expresso-query": "^0.11.6",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "i18next": "^23.11.5",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-middleware": "^3.6.0",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "pino-http": "^10.1.0",
    "pm2": "^5.4.0",
    "qs": "^6.12.1",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.1",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.1.65",
    "@swc/core": "1.3.78",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-useragent": "^1.0.5",
    "@types/hpp": "^0.2.6",
    "@types/lodash": "^4.17.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.2",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@types/qs": "^6.9.15",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-love": "^52.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.8.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "msw": "^2.3.1",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.1",
    "rimraf": "^5.0.7",
    "standard-version": "^9.5.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.4"
  }
}
